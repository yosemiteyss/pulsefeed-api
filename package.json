  "scripts": {
    "build": "nest build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest --config jest.json",
    "test:watch": "jest --config jest.json --watch",
    "test:cov": "jest --config jest.json --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config jest.json --runInBand",
    "migration:run": "npm run typeorm migration:run -- -d ormconfig.ts",
    "migration:generate": "nest build && npm run typeorm -- -d ormconfig.ts migration:generate ./migration/migration",
    "migration:create": "npm run typeorm -- migration:create ./migration/migration",
    "migration:revert": "npm run typeorm -- -d ormconfig.ts migration:revert"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/terminus": "^10.2.0",
    "@nestjs/throttler": "^5.1.1",
    "@nestjs/typeorm": "^10.0.1",
    "axios": "^1.6.3",
    "axios-cookiejar-support": "^4.0.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "crypto-ts": "^1.0.2",
    "dotenv": "^16.4.5",
    "ioredis": "^5.3.2",
    "moment": "^2.30.1",
    "nest-winston": "^1.9.4",
    "nestjs-http-logger": "^1.1.1",
    "pg": "^8.11.5",
    "pulsefeed-common": "file:pulsefeed-common",
    "ramda": "^0.29.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "tough-cookie": "^4.1.3",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
